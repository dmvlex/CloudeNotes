<Window x:Class="CloudNotes.RegistartionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CloudNotes"
        mc:Ignorable="d"
        Title="Регистрация в Я.Диск" Height="600" Width="400" ResizeMode="CanMinimize" Closed="WebWindowIsClosed" WindowStartupLocation="CenterScreen" ShowInTaskbar="False">
    <Grid>
        <Border BorderThickness="2" BorderBrush="#FF116ED6" Margin="5">
            <WebBrowser x:Name="LoginWebBrowser"
                    Margin="0" LoadCompleted="WebControlLoadComplete" Loaded="WebControlLoad"/>
        </Border>
    </Grid>
</Window>
